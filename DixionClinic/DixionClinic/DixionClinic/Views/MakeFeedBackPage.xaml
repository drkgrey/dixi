<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DixionClinic.MakeFeedBackPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="typeOfFeedBack" Margin="15,10,15,10" FontSize="Large" FontAttributes="Bold"/>
            <Editor x:Name="feedBack" HeightRequest="200" Margin="10,0,10,10"/>
            <Button x:Name="sendFeedBack" Text="Отправить отзыв" TextColor="White" BackgroundColor="MediumBlue"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>